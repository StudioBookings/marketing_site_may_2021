        <!-- End of Woopra Code -->
    {% endblock %}
</head>

<body class="{{ entry.slug }}{{ topBanner.show ? ' topBannerActive' : '' }}">

    <main class="main">

        <nav class="topbar">

            {% block topBanner %}
                {% include 'elements/topBanner.twig' %}
            {% endblock %}

            <div class="wrapper navigation-wrapper">

                <a href="/" class="logo" title="Studio Bookings - Fitness Studio Software, Yoga Studio Software">
                    <img src="images/studio-bookings-logo.svg" alt="Studio Bookings Logo">

                </a>

                <input type="checkbox" id="navtoggle">

                <div class="mobilenav-wrapper">

                    <ul class="pagenav">
                        <li>
                            <a href="/" title="Home"{% if entry.slug == 'main-content'  %} class="active"{% endif %}>Home</a>
                        </li>
                        <li>
                            <a href="features" title="Features"{% if entry.slug == 'features-page'  %} class="active"{% endif %}>Features</a>
                        </li>
                        <li>
                            <a href="pricing" title="Pricing"{% if entry.slug == 'pricing-page'  %} class="active"{% endif %}>Pricing</a>
                        </li>
                        <li>
                            <a href="faq" title="FAQ"{% if entry.slug == 'faq-page'  %} class="active"{% endif %}>FAQ</a>
                        </li>
                        <li>
                            <a href="app" title="APP"{% if entry.slug == 'app-page'  %} class="active"{% endif %}>APP</a>
                        </li>
                        <li>
                            <a href="http://help.studiobookings.com/" title="Help" target="_blank">Help</a>
                        </li>
                    </ul>
<a href="https://app.studiobookings.com/findyourstudio" title="Login" style="color: #008EFF;
opacity: 1;
width: 80px;
font-size: 16px;
padding: 5px 15px;">
                       Login
                    </a>


                    <a href="signup" title="Sign up for 14 days of free trial" class="button-full">
                        Free Trial
                    </a>

                    <div class="mobilenav-addon">
                        <p><i class="icon-lock-filled"></i> A credit card is not required to try us out</p>

                        <ul class="social-links">
                            <li>
                                <a href="https://twitter.com/studiobook" title="Hear us tweet on Twitter"><span class="icon-twitter"></span></a>
                            </li>
                            <li>
                                <a href="https://www.facebook.com/studiobookingsonline" title="Find us on Facebook"><span class="icon-facebook"></span></a>
                            </li>
                            <li>
                                <a href="https://www.instagram.com/studiobookingsonline/" title="Follow us on Instagram"><span class="icon-instagram"></span></a>
                            </li>
                        </ul>
                    </div>

                </div>

                <label class="navigation-button" for="navtoggle">
                    <i class="icon-nav"></i>
                </label>

            </div>
        </nav>

        {# individual page content here#}
        {% block mainContent %}{% endblock %}

    </main>

    {% block footerContent %}
    <footer class="pagefooter">

        {% block lastcta %}
        <div class="last-cta-box wrapper">
            <h3 class="headline-size-3">Build a stronger, healthier and more profitable studio with us!</h3>

            <div class="_buttons">
                <a href="signup" class="button-full" title="Try Us Free Now!">Try Us Free Now!</a>
                <a href="{{ videolink }}" data-width="640" data-ratio="16:9" data-fancybox class="button-simple" title="Watch the Video">Watch the Video <i class="icon-forward-arrow"><span class="last"></span></i></a>
            </div>
        </div>
        {% endblock %}

        <div class="wrapper footer-content">

            <div class="_box -about">

                <img src="images/studio-bookings-logo.svg" class="footer-logo" alt="Studio Bookings Logo">

                <p class="small-text">StudioBookings is a robust online studio management software that is used by thousands of businesses worldwide.</p>
            </div>

            <nav class="_box">
                <h4 class="headline-size-5">Sitemap</h4>
                <ul class="footer-links">
                    <li>
                        <a href="/" title="Home"{% if entry.slug == 'main-content'  %} class="active"{% endif %}>Home</a>
                    </li>
                    <li>
                        <a href="features" title="Features"{% if entry.slug == 'features-page'  %} class="active"{% endif %}>Features</a>
                    </li>
                    <li>
                        <a href="pricing" title="Pricing"{% if entry.slug == 'pricing-page'  %} class="active"{% endif %}>Pricing</a>
                    </li>
                    <li>
                        <a href="faq" title="FAQ"{% if entry.slug == 'faq-page'  %} class="active"{% endif %}>FAQ</a>
                    </li>
                    <li>
                        <a href="app" title="APP"{% if entry.slug == 'app-page'  %} class="active"{% endif %}>APP</a>
                    </li>
                </ul>
            </nav>

            <nav class="_box">
                <h4 class="headline-size-5">Our Offices</h4>
                <ul class="footer-links">
                    <li>
                        New York, NY
                    </li>
                    <li>
                        Boston, MA
                    </li>
                    <li>
                        Toronto, CAN
                    </li>
                    <li>
                        Melbourne, AUS
                    </li>
                </ul>
            </nav>

            <nav class="_box">
                <h4 class="headline-size-5">Connect With us</h4>
                <ul class="social-links">
                    <li>
                        <a href="https://twitter.com/studiobook" title="Hear us tweet on Twitter"><span class="icon-twitter"></span></a>
                    </li>
                    <li>
                        <a href="https://www.facebook.com/studiobookingsonline" title="Find us on Facebook"><span class="icon-facebook"></span></a>
                    </li>
                    <li>
                        <a href="https://www.instagram.com/studiobookingsonline/" title="Follow us on Instagram"><span class="icon-instagram"></span></a>
                    </li>
                </ul>
            </nav>

        </div>

        <div class="wrapper footer-end">
            <p>Copyright ® 2020 StudioBookingsOnline. All rights reserved. <a href="terms-of-use" title="Terms of Use">Terms of Use</a> | <a href="privacy-policy" title="Privacy Policy">Privacy Policy</a></p>

            <p>Made with ❤️ in Manhattan, New York, 10017.</p>
        </div>


    </footer>
    {% endblock %}

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script async defer src="scripts/main.js?v=2"></script>
