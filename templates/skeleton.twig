{% set videolink = 'video/studiobookings-animation.mp4' %}
{% set playstorelink = 'https://play.google.com/store/apps/details?id=com.studio&hl=en' %}
{% set appstorelink = 'https://itunes.apple.com/in/app/studiobookings/id971786033?mt=8' %}

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <base href="{{ siteUrl }}">

    <title>{{ metaTitle|default('StudioBookings - Fitness Studio Software, Yoga Studio Software') }}</title>
    <meta name="description" content="{{ metaDescription|default('StudioBookings is a robust online studio management software that is used by thousands of businesses worldwide.') }}">

    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta name="MobileOptimized" content="375">
    <meta name="HandheldFriendly" content="True">
    
    <link rel="stylesheet" type="text/css" href="styles/main.css?v=1.4">

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#2d89ef">
    <meta name="theme-color" content="#ffffff">

    <meta name="apple-itunes-app" content="app-id=971786033">

    <meta property="og:locale" content="en_US" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="{{ ogTitle|default('StudioBookings - Fitness Studio Software, Yoga Studio Software') }}" />
    <meta property="og:description" content="{{ ogDescription|default('StudioBookings offers the best and simplest yoga studio software to manage your yoga, pilates and cycling studio memberships online.') }}" />
    <meta property="og:url" content="{{ entry.url }}" />
    <meta property="og:image" content="{{ siteUrl ~ ogImage|default('images/og/studiobookings.png') }}" />
    <meta property="og:site_name" content="StudioBookingsOnline" />
    <meta name="twitter:card" content="summary" />
    <meta name="twitter:description" content="{{ twitterDescription|default('StudioBookings offers the best and simplest yoga studio software to manage your yoga, pilates and cycling studio memberships online.') }}" />
    <meta name="twitter:title" content="{{ twitterTitle|default('StudioBookings - Fitness Studio Software, Yoga Studio Software') }}" />
    <meta name="twitter:site" content="@studiobook" />
    <meta name="twitter:creator" content="@studiobook" />
    <meta name="google-site-verification" content="1u9qqAeo0W5YCGYh2TzHs55MH20yK40vOI6FXhj9buk" />

    {% block headScripts %}
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-35007929-1"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());

            gtag('config', 'UA-35007929-1');
        </script>
        <!-- Start of Woopra Code -->
        <script>
            (function(){
                var t,i,e,n=window,o=document,a=arguments,s="script",r=["config","track","identify","visit","push","call","trackForm","trackClick"],c=function(){var t,i=this;for(i._e=[],t=0;r.length>t;t++)(function(t){i[t]=function(){return i._e.push([t].concat(Array.prototype.slice.call(arguments,0))),i}})(r[t])};for(n._w=n._w||{},t=0;a.length>t;t++)n._w[a[t]]=n[a[t]]=n[a[t]]||new c;i=o.createElement(s),i.async=1,i.src="//static.woopra.com/js/w.js",e=o.getElementsByTagName(s)[0],e.parentNode.insertBefore(i,e)
